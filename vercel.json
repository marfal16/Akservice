{
  "builds": [
    {
      "src": "client/vite.config.js",
      "use": "@vercel/vite"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}
